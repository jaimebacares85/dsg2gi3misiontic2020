EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AACA,OARA;AASA,KAXA;AAYA,IAAA,eAZA,6BAYA;AAAA;;AAEA,WAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,KAAA,QAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AAEA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,QAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,kBAAA;;AACA,QAAA,MAAA,CAAA,SAAA;AAEA,OAdA,EAeA,KAfA,CAeA,UAAA,CAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AACA,OAlBA;AAoBA,KAlCA;AAqCA,IAAA,gBArCA,4BAqCA,gBArCA,EAqCA;AACA,WAAA,gBAAA,GAAA,gBAAA;AACA,KAvCA;AAwCA,IAAA,SAxCA,uBAwCA;AACA,WAAA,gBAAA,GAAA,KAAA,WAAA;AACA;AA1CA;AAdA,CAAA","sourcesContent":["<template>\n<section>\n<!--Inicio Formulario-->    \n    <div class=\"container-fluid bg-white\" >\n        <h2 class=\"text-center\">Inicio de Sesión o Registro</h2>\n            <div class=\"row justify-content-center\">\n                <div class=\"col-sm-12 col-xs-12 col-lg-12\"></div>       \n            </div>  \n    </div>\n    <hr>\n\n        <div class=\"contenedor__todo\">\n                <div class=\"caja__trasera\">\n                    <div class=\"caja__trasera-login\">\n                        <h3>¿Ya tienes una cuenta?</h3>\n                        <p>Inicia sesión para entrar en la página</p>\n                        <button id=\"btn__iniciar-sesion\">Iniciar Sesión</button>\n                    </div>\n                    <div class=\"caja__trasera-register\">\n                        <h3>¿Aún no tienes una cuenta?</h3>\n                        <p>Regístrate para que puedas iniciar sesión</p>\n                        <button id=\"btn__registrarse\">Regístrarse</button>\n                    </div>\n                </div>\n\n                <!--Formulario de Login y registro-->\n                <div class=\"contenedor__login-register\"> \n                    <!--Login-->\n                    <form action=\"\" class=\"formulario__login\">\n                        <h2>Iniciar Sesión</h2>\n                        <input type=\"text\" placeholder=\"Correo Electronico\">\n                        <input type=\"password\" placeholder=\"Contraseña\">\n                        <button>Entrar</button>\n                    </form>\n\n                    <!--Register-->\n                    <b-alert \n                    :show=\"dismissCountDown\" \n                    dismissible \n                    :variant=\"mensaje.color\" \n                    @dismissed=\"dismissCountDown=0\" \n                    @dismiss-count-down=\"countDownChanged\" > \n                    {{mensaje.texto}} \n                    </b-alert>\n\n                    <form @submit.prevent=\"agregarUsuarios()\" class=\"formulario__register\" v-for=\"(item, index) in usuarios\" :key=\"index\">\n                        <h2>Regístrarse</h2>\n\n                        <input type=\"text\" placeholder=\"Nombre completo\" v-model=\"usuarios.nombre\">\n                        <input type=\"email\" placeholder=\"Correo Electronico\" v-model=\"usuarios.correo\">\n                        <input type=\"text\" placeholder=\"Usuario\" v-model=\"usuarios.usuario\">\n                        <input type=\"text\" placeholder=\"Rol\" v-model=\"usuarios.rol\">\n                        <input type=\"text\" placeholder=\"Teléfono\" v-model=\"usuarios.telefono\">\n                        <input type=\"password\" placeholder=\"Contraseña\" v-model=\"usuarios.contraseña\"> \n                        <b-button class=\"btn-success my-2\" type=\"submit\">Regístrarse</b-button>\n\n\n                    </form>\n                </div>\n            </div>\n<!--Fin Formulario-->    \n<hr>\n\n<!--Inicio Tabla-->\n    <div class=\"container\">\n    <table class=\"table\">\n        <thead>\n        <tr>\n            <th scope=\"col\">#</th>\n            <th scope=\"col\">Nombre</th>\n            <th scope=\"col\">Correo</th>\n            <th scope=\"col\">Usuario</th>\n            <th scope=\"col\">Rol</th>\n            <th scope=\"col\">Telefono</th>\n            <th scope=\"col\">Contraseña</th>\n            <th scope=\"col\">Acciones</th>\n        </tr>\n        </thead>\n        <tbody>\n        <tr v-for=\"(item, index) in usuarios\" :key=\"index\">\n            <th scope=\"row\">{{item._id}}</th>\n            <td>{{item.nombre}}</td> \n            <td>{{item.correo}}</td>\n            <td>{{item.usuarioId}}</td>\n            <td>{{item.rol}}</td>\n            <td>{{item.telefono}}</td>\n            <td>{{item.contraseña}}</td>\n            <td>\n                <b-button class=\"btn-danger mx-2\" @click=\"eliminarUsuarios(item._id)\">Eliminar</b-button>\n                <b-button class=\"btn-warning mx-2\" @click=\"activarEdicion(item._id)\">Editar</b-button>\n\n            </td>\n        </tr>\n\n        </tbody>\n    </table>\n</div>\n<!--Fin Tabla-->\n</section>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            usuarios:[],\n            mensaje: {color: 'success', texto: ''}, \n            dismissSecs: 5, \n            dismissCountDown: 0,\n\n            usuarios:{nombre:\"\",correo:\"\",usuario:\"\",rol:\"\",telefono:\"\",contraseña:\"\"}\n        }\n    },\n    created() {\n        this.listarUsuarios();\n    },\n    methods:{\n        listarUsuarios(){\n            this.axios.get('/usuarios')\n            .then(res=>{\n                console.log(res.data)\n                this.usuarios=res.data;\n\n            })\n            .catch(e=>{\n                console.log(e.response)\n            })\n        },\n        agregarUsuarios(){\n\n            this.axios.post('/nuevo-registro',this.usuarios)\n            .then(res=>{\n\n                this.usuarios.push(res.data)\n                this.usuarios.nombre=\"\";\n                this.usuarios.correo=\"\";\n                this.usuarios.usuario=\"\";\n                this.usuarios.rol=\"\";\n                this.usuarios.telefono=\"\";\n                this.usuarios.contraseña=\"\";\n                this.mensaje.color=\"success\";\n                this.mensaje.texto=\"Usuario Agregado\";\n                this.showAlert();\n\n            })\n            .catch(e=>{\n\n                console.log(e.response)  \n            })\n\n        },\n\n        \n        countDownChanged(dismissCountDown) { \n            this.dismissCountDown = dismissCountDown \n        }, \n        showAlert() { \n            this.dismissCountDown = this.dismissSecs \n        }\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}